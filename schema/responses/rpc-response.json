{
  "$id": "http://www.pascalcoin.org/schemas/responses/rpc-response.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "This schema describes a basic JSON RPC response",
  "type": "object",
  "required": [
    "jsonrpc",
    "id",
    "result"
  ],
  "properties": {
    "jsonrpc": {
      "enum": [
        "2.0"
      ]
    },
    "id": {
      "type": [
        "string",
        "number",
        "null"
      ]
    }
  }
}